  "cds": {
    "requires": {
      "auth": {
        "kind": "mocked",
        "users": {
          "alice": {
            "roles": [
              "support"
            ]
          }
        }
      },
      "[production]": {
        "db": "hana",
        "auth": {
          "kind": "xsuaa"
        }
      },
      "destinations": true,
      "html5-runtime": true,
      "workzone": true
